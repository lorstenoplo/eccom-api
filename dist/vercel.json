{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "./(.*)",
      "methods": ["GET", "POST"],
      "headers": {
        "Access-Control-Allow-Origin": "https://goloop.vercel.app/",
        "Access-Control-Allow-Headers": "Origin, X-Requested-With, Content-Type, Accept",
        "Access-Control-Allow-Credentials": "true"
      },
      "dest": "index.js",
      "continue": true
    },
    {
      "src": "/(.*)",
      "methods": ["OPTIONS"],
      "dest": "/cors"
    }
  ]
}
